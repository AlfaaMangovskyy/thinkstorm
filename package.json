{
    "name": "thinkstorm",
    "displayName": "ThinkStorm",
    "publisher": "Patczatowicz",
    "description": "",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.73.0"
    },
    "categories": [
        "Themes"
    ],
    "activationEvents": [
        "onCommand:thinkstorm.evlet",
        "onStartupFinished",
        "onCommand:thinkstorm.dispend_note"
    ],
    "main": "./extension.js",
    "contributes": {
        "themes": [
            {
                "label": "ThinkStorm",
                "uiTheme": "vs-dark",
                "path": "./themes/ThinkStorm-color-theme.json"
            }
        ],
        "commands": [
            {
                "command": "thinkstorm.evlet",
                "title": "Theme Mixin'"
            },
            {
                "command": "thinkstorm.dispend_note",
                "title": "ThinkStorm : Change status bar notes."
            }
        ]
    },
    "icon": "icon.png",
    "repository": {
        "url": "https://github.com/AlfaaMangovskyy/thinkstorm"
    }
}